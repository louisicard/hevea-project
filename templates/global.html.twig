<!DOCTYPE html>
<html ng-app="HeveaApp">
<head>
  <meta charset="utf-8">
  <meta name="viewport" content="width=device-width, initial-scale=1, user-scalable=no">
  <meta name="theme-color" content="#f87a15">
  <base href="/">
  <title>Hevea project</title>
  <link rel='stylesheet' href='/stylesheets/main.css' />
  <link rel='stylesheet' href='/stylesheets/animate.css' />
  <link rel='stylesheet' href='assets/vendor/angular-material/angular-material.min.css' />
  <link rel="stylesheet" media="screen and (max-width: 1024px)" href="/stylesheets/rwd-1024.css" type="text/css" />
  <link rel="stylesheet" media="screen and (max-width: 450px)" href="/stylesheets/rwd-450.css" type="text/css" />
  <link rel="icon" href="/images/favicon.png" />
</head>
<body class="clearfix {{ '{{bodyClass}}' }} menu-{{ '{{menuClass}}' }}" ng-controller="GlobalController">
<header>
  <figure>
    <img src="images/logo-white.png" align="Logo Hevea" />
    <figcaption>Hevea</figcaption>
  </figure>
  <div class="menu-toggle-wrapper">
    <a ng-click="toggleMenu()"><i class="fa fa-bars" aria-hidden="true"></i></a>
  </div>
</header>
<div id="main-wrapper">
  <aside id="main-menu" class="{{ '{{menuClass}}' }}">
    <nav id="main-nav">
      <ul>
        <li><a ng-click="subMenu = null; rwdHide();" class="item-dashboard {{ '{{subMenu == null ? \'active\' : \'inactive\'}}' }}" href="{{ path('homepage') }}">{{ "{{ trans('nav.dashboard') }}" }}</a></li>
        <li>
          <a ng-click="subMenu = subMenu == 'datastudio' ? null : 'datastudio'" class="item-datastudio {{ '{{subMenu == \'datastudio\' ? \'active\' : \'inactive\'}}' }}">{{ "{{ trans('nav.datastudio.title') }}" }}</a>
          <ul class="{{ '{{subMenu == \'datastudio\' ? \'active\' : \'inactive\'}}' }}">
            <li><a href="{{ path('homepage') }}indices" ng-click="subMenu = 'datastudio'; rwdHide();">{{ "{{ trans('nav.datastudio.indices') }}" }}</a></li>
            <li><a ng-click="">{{ "{{ trans('nav.datastudio.datasources') }}" }}</a></li>
            <li><a ng-click="">{{ "{{ trans('nav.datastudio.matchingLists') }}" }}</a></li>
            <li><a ng-click="">{{ "{{ trans('nav.datastudio.processChains') }}" }}</a></li>
          </ul>
        </li>
        <li><a ng-click="" class="item-search">{{ "{{ trans('nav.search') }}" }}</a></li>
        <li><a ng-click="" class="item-tools">{{ "{{ trans('nav.tools') }}" }}</a></li>
        <li><a ng-click="" class="item-admin">{{ "{{ trans('nav.admin') }}" }}</a></li>
        <li><a ng-click="" href="{{ path('logout') }}" class="item-logout">{{ "{{ trans('nav.logout') }}" }}</a></li>
      </ul>
    </nav>
  </aside>
  <main ng-view autoscroll="true">
    <div class="loader-wrapper"><div class="loader"></div></div>
  </main>
</div>
</body>
<script>
  var __homepageUri = "{{ path('homepage') }}";
  var __i18n = {{ i18n|raw }};
</script>
<script src="https://use.fontawesome.com/30327982c3.js"></script>
<script src="assets/vendor/angular/angular.min.js"></script>
<script src="assets/vendor/angular-aria/angular-aria.js"></script>
<script src="assets/vendor/angular-route/angular-route.min.js"></script>
<script src="assets/vendor/angular-resource/angular-resource.min.js"></script>
<script src="assets/vendor/angular-animate/angular-animate.min.js"></script>
<script src="assets/vendor/angular-material/angular-material.js"></script>
<script src="assets/vendor/angular-sanitize/angular-sanitize.js"></script>
<script src="assets/vendor/jquery/dist/jquery.js"></script>
<script src="app/index.js"></script>
<script src="app/controllers/GlobalController.js"></script>
<script src="app/controllers/HomeController.js"></script>
<script src="app/controllers/IndicesController.js"></script>
<script src="app/controllers/IndexEditController.js"></script>
<script src="app/controllers/MappingController.js"></script>
</html>
